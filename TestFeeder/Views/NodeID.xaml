<Window x:Class="TestFeeder.Views.NodeID"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TestFeeder.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Width="250"
      Height="200"
      Title="NodeID" WindowStartupLocation="CenterScreen">

    <Grid>
        <DockPanel LastChildFill="false">
            <StackPanel HorizontalAlignment="Center" DockPanel.Dock="Top" >
                <Label Content="Enter Node ID:"></Label>
                <TextBox x:Name="node_id" PreviewTextInput="bandwidthTextBox_PreviewTextInput" Width="100" GotFocus="node_id_GotFocus" Margin="5"></TextBox>
                <Button Content="Create" Click="Button_Click" Width="100"></Button>
            </StackPanel>
        </DockPanel>
    </Grid>
</Window>
